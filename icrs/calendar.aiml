<aiml version="1.0.1" encoding="UTF-8">
   <topic name="ADDEVENT">
      <category>
          <pattern>ASKEVENT</pattern>
          <template>
            <condition>
              <li name="event-start" value=""><srai>ASKTIME</srai></li>
              <li name="event-end" value=""><srai>ASKENDTIME</srai></li>
              <li name="event-summary" value=""><srai>ASKSUMMARY</srai></li>
              <li name="event-attendees" value=""><srai>ASKATTENDEES</srai></li>
              <li><srai>ADDEVENT</srai></li>
            </condition>
          </template>
      </category>
      <category>
          <pattern>ASKTIME</pattern>
          <template>Sure, for what time?</template>
      </category>
      <category>
          <pattern>*</pattern>
          <that>SURE FOR WHAT TIME</that>
          <template>
            <think><set name="event-start"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>FOR *</pattern>
          <that>SURE FOR WHAT TIME</that>
          <template>
            <think><set name="event-start"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>ASKENDTIME</pattern>
          <template>And you would end by what time?</template>
      </category>
      <category>
          <pattern>AROUND *</pattern>
          <that>AND YOU WOULD END BY WHAT TIME</that>
          <template>
            <think><set name="event-end"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>*</pattern>
          <that>AND YOU WOULD END BY WHAT TIME</that>
          <template>
            <think><set name="event-end"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>ASKSUMMARY</pattern>
          <template>And what shall we name this event ?</template>
      </category>
      <category>
          <pattern>CALL IT *</pattern>
          <that>AND WHAT SHALL WE NAME THIS EVENT</that>
          <template>
            <think><set name="event-summary"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>*</pattern>
          <that>AND WHAT SHALL WE NAME THIS EVENT</that>
          <template>
            <think><set name="event-summary"><star/></set></think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>* WITH *</pattern>
          <that>AND WHAT SHALL WE NAME THIS EVENT</that>
          <template>
            <think>
              <set name="event-summary"><star index="1"/> with <star index="2"/></set>
              <set name="event-friend"><star index="2"/></set>
            </think>
            <srai>ASKEVENT</srai>
          </template>
      </category>
      <category>
          <pattern>_ WHATEVER *</pattern>
          <that>AND WHAT SHALL WE NAME THIS EVENT</that>
          <template>
            <think><set name="event-summary">Unnamed Event</set></think>
          </template>
      </category>
      <category>
          <pattern>ASKATTENDEES</pattern>
          <template>Who else should we invite ?</template>
      </category>
      <category>
          <pattern>*</pattern>
          <that>WHO ELSE SHOULD WE INVITE</that>
          <template>
            <srai>ADDATTENDEES <star/></srai>
          </template>
      </category>
      <category>
          <pattern>INVITE *</pattern>
          <that>WHO ELSE SHOULD I INVITE</that>
          <template>
            <srai>ADDATTENDEES <star/></srai>
          </template>
      </category>
      <category>
          <pattern>ADDATTENDEES *</pattern>
          <template>
            <think>
              <set name="event-attendee"><get name="event-attendee"/>,<star/></set>
            </think>
          </template>
      </category>
      <category>
          <pattern>ASKFRIENDEMAIL *</pattern>
          <template>May I have <star/>'s email ?</template>
      </category>
      <category>
          <pattern>*</pattern>
          <that>MAY I HAVE * EMAIL</that>
          <template>
            <srai>ADDATTENDEES <star/></srai>
          </template>
      </category>
      <category>
          <pattern>NO *</pattern>
          <that>MAY I HAVE * EMAIL</that>
          <template>No problems. </template>
      </category>
      <category>
          <pattern>ADDEVENT</pattern>
          <template>
            <system>ruby ./executables/google-apis.rb add --start <get name="event-start"/> --end <get name="event-end"/> --summary <get name="event-summary"/> --attendees <get name="event-attendees"/></system>
          </template>
      </category>
    </topic>
